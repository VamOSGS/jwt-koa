{
    "extends": "airbnb-base",
    "globals": {
        "global": true
    },
    "env": {
        "node": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-undef": "off",
        "no-console": "off",
        "import/no-extraneous-dependencies": "off",
        "no-unused-vars": "warn",
        "import/no-dynamic-require": "off",
        "linebreak-style": "off"
    }
}
